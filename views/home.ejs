<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Fav button test</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
    <script src="https://kit.fontawesome.com/77de315972.js" crossorigin="anonymous"></script>
  </head>

  <body>
    <div class="favourites-item">
      <% for (let item of items) { %>

        <div class="item">
          <a href="http://localhost:8080/home/listings/<%=item.id%>">
          <p><%= item.title %></p>
          <img src="<%= item.cover_picture_url %>"/>
          <p><%= ("$" + item.price) %></p>
        </a>
        </div>
        <i class="far fa-heart" data-item="<%= item.id %>" data-id="far-fa-heart" id="fas-fa-heart"></i>
        <% } %>


      </div>
  </body>
</html>
